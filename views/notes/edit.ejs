<h2 class="page-header">Edit Note</h2>
<form action="/notes/<%= note.id %>?_method=PUT" method="POST">
  <%- include('_form_field') %>
  <div class="form-row form-row-end btn-row">
    <a class="btn btn-danger" href="/notes/<%= note.id %>">Cancel</a>
    <button class="btn btn-primary" type="submit">Update</button>
  </div>
</form>